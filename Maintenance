Set-ADGroup -Identity "maintenance.toulouse-peira-rosa" -Add @{msExchHideFromAddressLists=$true}

Set-ADGroup -Identity "maintenance.toulouse-peira-rosa" -Add @{proxyAddresses = @("SMTP:maintenance.toulouse-peira-rosa@domitys.fr")}

Set-ADGroup -Identity "maintenance.toulouse-peira-rosa" -Add @{displayName = @("LD - Maintenance - Toulouse / Peira Rosa")}


